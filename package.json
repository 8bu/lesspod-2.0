{
  "name": "lesspod-2.0",
  "version": "1.0.0",
  "description": "Lesspod-2.0 built with vue/nuxt/serverless",
  "author": "Rajan Chandi",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build": "cross-env NODE_ENV=production nuxt build",
    "sls:local": "sls offline",
    "sls:create": "npm run build && sls create_domain && sls deploy",
    "sls:delete": "sls remove && sls delete_domain",
    "sls:deploy": "npm run build && sls deploy"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "@nuxtjs/axios": "^5.0.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "nuxt": "^2.2.0",
    "nuxt-property-decorator": "^1.2.0",
    "postcss": "^7.0.11",
    "serverless-http": "^1.7.2",
    "tailwindcss": "^0.7.3",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "prettier": "1.14.3",
    "autoprefixer": "^8.6.4",
    "tailwindcss": "~0.6.6",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "serverless": "^1.32.0",
    "serverless-domain-manager": "^2.6.5",
    "serverless-offline": "^3.29.0",
    "serverless-s3-sync": "^1.7.0"
  }
}
