{
  "name": "lesspod-2.0",
  "version": "1.0.0",
  "description": "Lesspod-2.0 built with vue/nuxt/serverless",
  "author": "Rajan Chandi",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-autofix": "eslint --ext .js,.vue src test --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "build": "cross-env NODE_ENV=production nuxt build",
    "sls:local": "sls offline start --skipCacheInvalidation",
    "sls:create": "npm run build && sls create_domain && sls deploy",
    "sls:delete": "sls remove && sls delete_domain",
    "sls:deploy": "npm run build && sls deploy"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.2.1",
    "bootstrap-vue": "^2.0.0-rc.11",
    "bootswatch": "^4.2.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dot-env": "0.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "highlight.js": "^9.14.2",
    "husky": "^1.3.1",
    "jquery": "^3.3.1",
    "mongoose": "^5.4.8",
    "nuxt": "^2.4.2",
    "nuxt-property-decorator": "^1.3.1",
    "popper.js": "^1.14.7",
    "postcss": "^7.0.14",
    "reading-time": "^1.1.3",
    "serverless-http": "^1.9.0",
    "style-loader": "^0.23.1",
    "terser": "^3.14.1",
    "vue": "^2.5.22",
    "vue-moment": "^4.0.0",
    "vue-quill-editor": "^3.0.6",
    "vue-sidebar-menu": "^3.6.0",
    "vuex-class": "^0.3.1",
    "vuex-map-fields": "^1.3.2",
    "vuex-persist": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-nuxt": "^0.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-script-hook": "^1.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.3",
    "sass-loader": "^7.1.0",
    "serverless": "^1.36.3",
    "serverless-domain-manager": "^2.6.13",
    "serverless-offline": "^4.2.1",
    "serverless-s3-sync": "^1.7.1",
    "tailwindcss": "^0.7.4"
  }
}
